<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/data/field/voting/db" VERSION="20190401" COMMENT="XMLDB file for Moodle mod/data/field/voting"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="data_votings" COMMENT="voting data">
            <FIELDS>
                <FIELD NAME="id" SEQUENCE="true" TYPE="int" LENGTH="11" NOTNULL="true" />
                <FIELD NAME="recordid" SEQUENCE="false" TYPE="int" LENGTH="11" NOTNULL="true" />
                <FIELD NAME="fieldid" SEQUENCE="false" TYPE="int" LENGTH="11" NOTNULL="true" />
                <FIELD NAME="userid" SEQUENCE="false" TYPE="int" LENGTH="11" NOTNULL="true" />
                <FIELD NAME="timecreated" SEQUENCE="false" TYPE="int" LENGTH="11" NOTNULL="true" />
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" />
                <KEY NAME="recordid" TYPE="foreign" FIELDS="recordid" REFTABLE="data_records" REFFIELDS="id" />
                <KEY NAME="fieldid" TYPE="foreign" FIELDS="fieldid" REFTABLE="data_fields" REFFIELDS="id" />
                <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id" />
            </KEYS>
            <INDEXES>
                <INDEX NAME="vote_idx" UNIQUE="true" FIELDS="recordid, fieldid, userid" />
            </INDEXES>
        </TABLE>
    </TABLES>
</XMLDB>
